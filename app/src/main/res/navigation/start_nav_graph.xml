<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/start_nav_graph"
    app:startDestination="@id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="gent.zeus.tappb.ui.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_nav_home_to_shoppingCartFragment"
            app:destination="@id/shoppingCartFragment" />
    </fragment>
    <fragment
        android:id="@+id/nav_stock"
        android:name="gent.zeus.tappb.ui.StockFragment"
        android:label="StockFragment"
        tools:layout="@layout/fragment_stock"/>
    <fragment
        android:id="@+id/nav_money"
        android:name="gent.zeus.tappb.ui.MoneyFragment"
        android:label="MoneyFragment"
        tools:layout="@layout/fragment_money"/>
    <fragment
        android:id="@+id/shoppingCartFragment"
        android:name="gent.zeus.tappb.ui.ShoppingCartFragment"
        android:label="ShoppingCartFragment" />

</navigation>